    "build": "parcel build src/clickup.ts -d clickup-gfmeditor && parcel build style/style.scss -d clickup-gfmeditor",
    "tslint": "tslint --fix src/**/*.{ts,tsx}",
    "prettier": "prettier --write src/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/redux": "^3.6.0",
    "marked-ts": "^1.0.0-beta.4",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "sass": "^1.21.0",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1"
  }
}
